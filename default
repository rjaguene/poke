server {
        listen 80 default_server;
        listen [::]:80 default_server;      
  
        #provide the build path
        root /var/www/html/poke/dist/poke/;
        index index.html index.htm index.nginx-debian.html;
        server_name _;
        location / {
             try_files $uri $uri/ /index.html;
        }
}